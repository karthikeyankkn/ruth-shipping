<div class="container-fluid">
	<%= form_for @import do |f| %>
		<div class="mt-20">
			<%= render 'shared/error_messages', object: f.object %>
		</div>	
		<div class="row">
			<div class="col-sm-3 mt-20">
		  	<label>Job No:</label>
		  	<%= f.number_field :job_no, placeholder: "Enter Job Number", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>IMP:</label>
		  	<%= f.text_field :imp, placeholder: "Enter IMP", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Supplier Name:</label>
		  	<%= f.text_field :supplier_name, placeholder: "Enter Supplier Name", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Cargo Name:</label>
		  	<%= f.text_field :cargo_name, placeholder: "Enter Cargo Name", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Port of Loading:</label>
		  	<%= f.text_field :port_of_loading, placeholder: "Enter Port of Loading", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Line:</label>
		  	<%= f.text_field :line, placeholder: "Enter Line", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>BL Number:</label>
		  	<%= f.text_field :bl_no, placeholder: "Enter ", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>status:</label>
		  	<%= f.select :status, options_for_select(["exam over", "Duty paid", "riginal/check", "COPY", "original", "ready duty"], :selected => f.object.status) %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>20':</label>
		  	<%= f.text_field :_20_inch, placeholder: "Enter 20'", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>40':</label>
		  	<%= f.text_field :_40_inch, placeholder: "Enter 40'", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>VSL Name:</label>
		  	<%= f.text_field :vsl_name, placeholder: "Enter VSL Name", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Free Days:</label>
		  	<%= f.text_field :free_days, placeholder: "Enter Free Days", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Lot No:</label>
		  	<%= f.number_field :lot_no, placeholder: "Enter Lot Number", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Shifting:</label>
		  	<%= f.number_field :shifting, placeholder: "Enter Shifting", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Weight:</label>
		  	<%= f.number_field :weight, placeholder: "Enter Weight", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Arrived:</label>
		  	<%= f.date_field :arrived, placeholder: "Enter Arrived", class:"form-control" %>
		  </div>
		  <div class="col-sm-3 mt-20">
		  	<label>Dem Upto:</label>
		  	<%= f.date_field :dem_upto, placeholder: "Enter Dem Upto", class:"form-control" %>
		  </div>

		  <div class="col-sm-12 mt-20 text-center">
		  	<%= f.submit "Post", class: "btn btn-primary" %>
		  </div>
		</div>
	<% end %>
</div>
